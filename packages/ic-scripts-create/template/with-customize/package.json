{
  "name": "{%=name%}",
  "version": "0.1.0",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --config .prettierrc --write",
      "git add"
    ]
  },
  "scripts": {
    "start": "run-all start",
    "docker-start": "egg-scripts start",
    "build": "cross-env CI=false run-all build",
    "prettier": "prettier --config .prettierrc --write 'src/**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "cache": "customize-cache",
    "test": "run-scripts test"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 8",
    "not op_mini all"
  ],
  "dependencies": {
    "egg": "^2.13.0",
    "egg-api-proxy": "^2.4.5",
    "egg-scripts": "^2.10.0",
    "egg-view-nunjucks": "^2.2.0",
    "egg-virtual-static": "^2.4.5",
    "lodash": "^4.17.11",
    "querystring": "^0.2.0"
  },
  "devDependencies": {
    "@engr/ic-customize-cache": "^2.0.0",
    "@engr/ic-scripts": "^2.0.0",
    "@engr/ic-scripts-all": "^2.0.0",
    "@engr/ic-scripts-rewired-customize": "^2.0.0",
    "@engr/ic-scripts-rewired-sass2customize": "^2.0.0",
    "@engr/ic-scripts-rewired-server": "^2.0.0",
    "egg-api-mock": "^2.4.5",
    "egg-bin": "^4.9.0",
    "cross-env": "^5.2.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "node-sass": "^4.10.0",
    "prettier": "^1.15.3",
    "react": "^16.7.0",
    "react-app-polyfill": "^0.1.3",
    "react-dom": "^16.7.0",
    "react-router-dom": "^4.3.1"
  }
}
